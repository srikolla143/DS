class Stack:
    def __init__(self):
        self.data=[]
    def push(self,val):
        self.data.append(val)
    def delete(self):
        if(self.data!=[]):
            print(self.data.pop())
    def top(self):
        if self.data!=[]:
            print(self.data[-1])
    def isEmpty(self):
        return self.data.empty()
    def display(self):
        return self.data
mystack=Stack()
mystack.push(10)
mystack.push(20)
mystack.push(30)
mystack.push(40)
mystack.delete()
mystack.top()
